function [a,b] = EdgeTest(Distro,j)
%3 cases
% 
% Moves(:,[1:6],2) = [0 0 1 1 1 0 ; 0 1 1 0 -1 -1];% 6a < -------  2
% Moves(:,[1:6],4) = [0 1 1 0 -1 -1; 0 0 -1 -1 -1 0];% 6c < -------   4
% ` = [0 1 1 0; 0 0 -1 -1];% 4a < ---------  6
%
a = 9;
b = 1;
A = Distro(j,:);
if (((A(1,1) - 1) < 1) || ((A(1,2) - 1) < 1)) == 1
    1
end
if (((A(1,1) - 1) < 1) || ((A(1,2) - 1) < 1))
    a = 4;
    b = 6;
end